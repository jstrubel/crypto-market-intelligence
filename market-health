export default function handler(req, res) {
  if (req.method === 'POST') {
    const data = req.body;
    console.log('Market Health webhook received:', data);
    
    // For now just log it - we'll add storage next
    res.status(200).json({ success: true, received: data });
  } else {
    res.status(405).json({ error: 'Method not allowed' });
  }
}
